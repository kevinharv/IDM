{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "module": "ES6",
    "outDir": "./dist",
    "baseUrl": "./src",
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "paths": {
      "@/*": ["./src/*"],
      "@routes/*": ["./src/routes/*"]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}